{
  "name": "Google Meet Refresh Prevent",
  "description": "Never accidentally refresh Google Meet tab again!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/icons/16x16.png",
    "32": "/icons/32x32.png",
    "64": "/icons/64x64.png",
    "128": "/icons/128x128.png",
    "256": "/icons/256x256.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["*://meet.google.com/*"],
      "run_at": "document_end"
    }
  ]
}
